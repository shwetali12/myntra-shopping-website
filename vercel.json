{
    "version":2,
    "builds":[
        {
            "src":"myntra/wsgi.py",
            "use":"@vercel/python",
            "config":{"maxLambdaSize": "15mb", "runtime":"python3.9"}
        },
        {
            "src":"build_files.sh",
            "use":"@vercel/static-build",
            "dest":"/static/$1"
        },
        {
            "src":"/(.*)",
            "dest":"myntra/wsgi.py"
        }
    ]
}